{
    "supported_languages": [
        {
            "language" : "Java",
            "file_extension" : ".java",
            "mime_type" : "text/x-java-source",
            "terminator" : ";"
        },
        {
            "language" : "Python",
            "file_extension" : ".py",
            "mime_type" : "text/x-python",
            "code_word_regexes" : ["assert", "async", "await", "b\"", "bool", "break", "continue", "def", "del", "elif", "False", 
            "global", "import", "import [a-z] as", "is None", "is not", "nonlocal", "None", "not in", "pass", "raise", "range", 
            "return", "self", "str", "True", "with [a-z] as", "yield"]
        },
        {
            "language" : "C",
            "file_extension" : [".c", ".h"],
            "mime_type" : "text/x-c",
            "terminator" : ";"
        }      
    ]
}